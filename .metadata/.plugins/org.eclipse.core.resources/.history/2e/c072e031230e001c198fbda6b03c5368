
public class InstructorManager {
	public void open(Instructor instructor) {
		System.out.println(instructor.getInstructorNumber() + " numaralý eðitmen " +  instructor.getFirstName());
		for (String course : instructor.getCoursesGiven()) {
			System.out.print(", " + course);
		}
		System.out.println(" isimli kurslarý vermeye baþladý.");
	}
}
