
public class Main {

	public static void main(String[] args) {
		Instructor instructor = new Instructor();
		instructor.setId(1);
		instructor.setFirstName("Engin");
		instructor.setLastName("Demiroğ");
		instructor.setInstructorNumber("12346789");
		instructor.setNameOfCourseGiven("JAVA");

		Student student = new Student();
		student.setId(1);
		student.setFirstName("Tolga");
		student.setLastName("Kümbül");
		student.setStudentNumber("987654321");
		student.setNameOfCourseTaken("JAVA");

		Course course = new Course();
		course.setId(1);
		course.setCourseName("Java");
		course.setInstructorName("Engin Demiroğ");
		CourseManager courseManager = new CourseManager();
		courseManager.add(course);
		InstructorManager instructorManager = new InstructorManager();
		instructorManager.open(instructor);
		StudentManager studentManager = new StudentManager();
		studentManager.join(student);

	}

}
